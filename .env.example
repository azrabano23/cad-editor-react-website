# HoloDraft Environment Variables
# Copy this file to .env and fill in your actual values

# Supabase Configuration
REACT_APP_SUPABASE_URL=your-project-url.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your-anon-key-here

# Backend API Configuration
REACT_APP_API_URL=http://localhost:5000

# File Upload Configuration
REACT_APP_MAX_FILE_SIZE=52428800  # 50MB in bytes
REACT_APP_SUPPORTED_FORMATS=stl,step,obj,ply,dae

# AR/MetaQuest Configuration
REACT_APP_AR_VIEWER_URL=your-ar-viewer-url

# Analytics (Optional)
REACT_APP_ANALYTICS_ID=your-analytics-id
